{
  "name": "redis-time-series",
  "version": "1.0.0",
  "description": "Javascript RedisTimeSeries client",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest -i --config jest.config.js",
    "test:watch": "jest -i --config jest.config.js --coverage --watchAll",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint \"src/**\"",
    "prepare" : "npm run build",
    "prepublishOnly" : "npm test && npm run lint",
    "preversion" : "npm run lint",
    "version" : "npm run format && git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/averias/redis-time-series.git"
  },
  "keywords": [
    "redis",
    "redistimeseries",
    "redislabs",
    "timeseries",
    "javascript",
    "typescript"
  ],
  "author": "Rafa Campoy",
  "license": "MIT",
  "homepage": "https://github.com/averias/redis-time-series",
  "dependencies": {
    "ioredis": "^4.14.1"
  },
  "devDependencies": {
    "@types/ioredis": "^4.0.20",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.3"
  },
  "files": ["lib/**/*"]
}
